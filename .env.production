# Production Environment Variables for Vercel
# These will be overridden by Vercel's environment variables

# App Configuration
VITE_APP_NAME=GymGenie
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION="AI-Powered Fitness Tracker"

# API Configuration
# VITE_GEMINI_API_KEY will be set in Vercel dashboard
VITE_API_TIMEOUT=30000

# Feature Flags
VITE_ENABLE_PWA=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_AI_FEATURES=true

# Performance
VITE_ENABLE_SOURCE_MAPS=false
VITE_ENABLE_BUNDLE_ANALYZER=false

# Security
VITE_ENABLE_CSP=true
VITE_ENABLE_HTTPS_ONLY=true
